    "c12": "^1.4.2",
    "consola": "^3.2.3",
    "debug": "^4.3.4",
    "defu": "^6.1.2",
    "fs-extra": "^11.1.1",
    "global-dirs": "^3.0.1",
    "gray-matter": "^4.0.3",
    "import-from": "^4.0.0",
    "is-installed-globally": "^0.4.0",
    "jiti": "^1.19.1",
    "kolorist": "^1.8.0",
    "markdown-it-link-attributes": "^4.0.1",
    "open": "^8.4.2",
    "prompts": "^2.4.2",
    "resolve": "^1.22.2",
    "resolve-from": "^5.0.0",
    "resolve-global": "^1.0.0",
