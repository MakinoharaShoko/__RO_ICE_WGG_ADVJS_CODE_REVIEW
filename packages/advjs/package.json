    "vue-router": "^4.2.4",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/markdown-it-link-attributes": "^3.0.1",
    "fast-deep-equal": "^3.1.3",
    "hast-util-to-html": "^8.0.4",
    "mdast-util-to-hast": "^12.3.0",
    "semver": "^7.5.4"
  }
