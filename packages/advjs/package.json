{
  "name": "advjs",
  "version": "0.0.1",
  "description": "A parser for @advjs.",
  "author": {
    "name": "YunYouJun",
    "email": "me@yunyoujun.cn",
    "url": "https://www.yunyoujun.cn"
  },
  "license": "MPL-2.0",
  "funding": "https://github.com/sponsors/YunYouJun",
  "repository": {
    "url": "https://github.com/YunYouJun/advjs"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "adv": "./bin/adv.js",
    "advjs": "./bin/adv.js",
    "advm": "./bin/adv.mjs"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "build": "rimraf dist && tsup node/index.ts node/cli.ts --dts --format cjs,esm --splitting --external @advjs/client/package.json",
    "build:client": "cross-env NODE_ENV=production vite build",
    "dev": "nr build --watch",
    "prepublishOnly": "npm run build",
    "start": "esno node/cli.ts"
  },
  "dependencies": {
    "@advjs/client": "workspace:*",
    "@advjs/core": "workspace:*",
    "@advjs/parser": "workspace:*",
    "@advjs/theme-default": "workspace:*",
    "@advjs/types": "workspace:*",
    "@babylonjs/core": "^4.2.2",
    "@babylonjs/gui": "^4.2.2",
    "@babylonjs/loaders": "^4.2.2",
    "@iconify-json/carbon": "^1.1.5",
    "@iconify-json/ri": "^1.1.2",
    "@intlify/vite-plugin-vue-i18n": "^3.4.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@windicss/plugin-animations": "^1.0.9",
    "babylon-vrm-loader": "^1.6.0",
    "fs-extra": "^10.1.0",
    "global-dirs": "^3.0.0",
    "import-from": "^4.0.0",
    "is-installed-globally": "^0.4.0",
    "jiti": "^1.13.0",
    "markdown-it-link-attributes": "^4.0.0",
    "nprogress": "^0.2.0",
    "open": "^8.4.0",
    "pinia": "^2.0.14",
    "prompts": "^2.4.2",
    "resolve": "^1.22.0",
    "resolve-from": "^5.0.0",
    "resolve-global": "^1.0.0",
    "unplugin-auto-import": "^0.8.6",
    "unplugin-icons": "^0.14.3",
    "unplugin-vue-components": "^0.19.6",
    "vite": "^2.9.9",
    "vite-plugin-md": "^0.13.1",
    "vite-plugin-pages": "^0.23.0",
    "vite-plugin-pwa": "^0.12.0",
    "vite-plugin-vue-layouts": "^0.6.0",
    "vite-plugin-windicss": "^1.8.4",
    "vite-ssg": "^0.20.1",
    "vue": "^3.2.37",
    "vue-i18n": "^9.1.10",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@babylonjs/materials": "^4.2.2",
    "@types/markdown-it-link-attributes": "^3.0.1",
    "@types/nprogress": "^0.2.0",
    "hast-util-to-html": "^8.0.3",
    "mdast-util-to-hast": "^12.1.1"
  }
}
