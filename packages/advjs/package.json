{
  "name": "advjs-monorepo",
  "version": "0.0.1",
  "description": "A parser for @advjs.",
  "main": "dist/index.js",
  "scripts": {
    "build": "vite build",
    "build:parser": "cd packages/parser && yarn build",
    "dev": "vite",
    "dev:parser": "cd packages/parser && yarn dev",
    "prepublishOnly": "yarn build",
    "start": "yarn dev",
    "lint": "eslint . --ext .vue,.ts,.tsx"
  },
  "devDependencies": {
    "@types/marked": "^1.2.1",
    "@types/node": "^14.14.22",
    "@types/prismjs": "^1.16.2",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "@vitejs/plugin-vue": "^1.1.4",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vuedx/typescript-plugin-vue": "^0.6.3",
    "autoprefixer": "^10.2.4",
    "eslint": "^7.19.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.5.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "postcss": "^8.2.4",
    "prettier": "^2.2.1",
    "prismjs": "^1.23.0",
    "sass": "^1.32.5",
    "tailwindcss": "^2.0.2",
    "typescript": "^4.1.3",
    "vite": "^2.0.0-beta.60"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{vue,ts,tsx}": [
      "yarn lint",
      "git add"
    ],
    "*.{ts,tsx}": "eslint --cache --fix"
  },
  "author": {
    "name": "YunYouJun",
    "email": "me@yunyoujun.cn",
    "url": "https://www.yunyoujun.cn"
  },
  "license": "MPL-2.0",
  "dependencies": {
    "marked": "^1.2.8",
    "register-service-worker": "^1.7.2",
    "typed.js": "^2.0.11",
    "vue": "^3.0.5",
    "vue-router": "^4.0.3",
    "vuex": "^4.0.0-rc.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
