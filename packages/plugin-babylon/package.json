{
  "name": "@advjs/plugin-babylon",
  "version": "0.1.0",
  "description": "Babylon Plugin for ADV.JS to support 3D",
  "author": {
    "name": "YunYouJun",
    "email": "me@yunyoujun.cn",
    "url": "https://www.yunyoujun.cn"
  },
  "license": "MPL-2.0",
  "homepage": "https://github.com/YunYouJun/advjs/tree/main/packages/plugin-babylon#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YunYouJun/advjs.git"
  },
  "bugs": {
    "url": "https://github.com/YunYouJun/advjs/issues"
  },
  "keywords": [
    "advjs",
    "babylon",
    "vrm"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./*": "./*"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "*.d.ts",
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "prepublishOnly": "npm run build",
    "start": "tsx src/index.ts",
    "test": "vitest"
  },
  "dependencies": {
    "@advjs/types": "workspace:*",
    "@babylonjs/core": "^5.57.1",
    "@babylonjs/gui": "^5.57.1",
    "@babylonjs/loaders": "^5.57.1",
    "@babylonjs/materials": "^5.57.1",
    "babylon-vrm-loader": "^2.0.0"
  },
  "devDependencies": {
    "tsup": "^7.2.0"
  }
}
