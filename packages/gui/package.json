{
  "name": "@advjs/gui",
  "type": "module",
  "version": "0.0.3-beta.6",
  "repository": {
    "url": "https://github.com/YunYouJun/advjs/tree/main/packages/gui",
    "type": "git"
  },
  "exports": {
    "./components/*": "./client/components/*",
    "./nuxt": "./nuxt.mjs",
    "./node": {
      "types": "./dist/node/index.d.ts",
      "import": "./dist/node/index.mjs",
      "require": "./dist/node/index.cjs"
    },
    ".": {
      "import": "./dist/agui.js",
      "require": "./dist/agui.umd.cjs"
    },
    "./*": "./*"
  },
  "main": "./dist/agui.umd.cjs",
  "module": "./dist/agui.js",
  "types": "./dist/client/types/index.d.ts",
  "scripts": {
    "build": "npm run build:lib && npm run build:icons && npm run build:node",
    "build:node": "unbuild",
    "build:lib": "vue-tsc --noEmit && vite build",
    "build:icons": "tsx scripts/build.ts",
    "prepublishOnly": "npm run build",
    "release": "bumpp --no-commit --no-tag --no-push && npm publish",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.17",
    "@iconify-json/vscode-icons": "^1.1.32",
    "@iconify/vue": "^4.1.1",
    "@radix-ui/colors": "^3.0.0",
    "radix-vue": "^1.2.7",
    "splitpanes": "^3.1.5"
  },
  "devDependencies": {
    "@types/splitpanes": "^2.2.6",
    "vite-plugin-css-injected-by-js": "^3.3.1",
    "vue-tsc": "^1.8.27"
  }
}
