{
  "name": "advjs",
  "version": "0.0.1",
  "description": "A parser for @advjs.",
  "main": "dist/index.js",
  "devDependencies": {
    "@types/marked": "^1.1.0",
    "@types/prismjs": "^1.16.1",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "@vue/compiler-sfc": "^3.0.0-0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "eslint": "^7.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^7.0.1",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "prismjs": "^1.21.0",
    "sass": "^1.27.0",
    "tailwindcss": "^1.8.13",
    "typescript": "^4.0.3",
    "vite": "^1.0.0-rc.4"
  },
  "scripts": {
    "build": "vite build",
    "build:parser": "cd packages/parser && yarn build",
    "dev": "vite",
    "dev:parser": "cd packages/parser && yarn dev",
    "prepublishOnly": "yarn build",
    "start": "yarn dev",
    "lint": "eslint . --ext .vue,.ts,.tsx"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{vue,ts,tsx}": [
      "yarn lint",
      "git add"
    ],
    "*.{ts,tsx}": "eslint --cache --fix"
  },
  "author": {
    "name": "YunYouJun",
